[cisco:children]
routers
switches

[routers]
R1 ansible_host=192.168.1.1 loopback=10.0.0.1
R2 ansible_host=192.168.1.2 loopback=10.0.0.2

[switches]
S1 ansible_host=192.168.1.3
S2 ansible_host=192.168.1.4

[cisco:vars]
ansible_connection=ansible.netcommon.network_cli
ansible_network_os=cisco.ios.ios
ansible_user=admin
ansible_password=cisco
ansible_become=yes
ansible_become_method=enable
ansible_become_password=class
